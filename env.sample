# Nutrient Web SDK Configuration
NEXT_PUBLIC_WEB_SDK_VERSION=1.7.0
NEXT_PUBLIC_WEB_SDK_LICENSE_KEY=your_web_sdk_license_key_here

# Default PDF Document URL (optional)
NEXT_PUBLIC_DEFAULT_DOCUMENT_URL=https://example.com/sample.pdf

# Nutrient Document Engine (DWS) Configuration
# Note: Keep these variables server-side only (without NEXT_PUBLIC_ prefix)
# The API key should NEVER be exposed to the client
DOCUMENT_ENGINE_SERVER_URL=https://your-dws-instance.com
DOCUMENT_ENGINE_API_KEY=your_api_key_here

# Document ID should be public as it's needed by the client to request a JWT
NEXT_PUBLIC_DOCUMENT_ENGINE_DOCUMENT_ID=your_document_id_here